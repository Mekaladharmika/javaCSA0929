public class datatypes
{
    public static void main(String args[])
    {
        int i=10;
        float f=2.8f;
        double d=3.123;
        char c='p';
        String s="pooja";
        boolean b=true;
        System.out.println("i = "+i);
        System.out.println("f = "+f);
        System.out.println("d = "+d);
        System.out.println("c = "+c);
        System.out.println("s = "+s);
        System.out.println("b = "+b);
    }

}
